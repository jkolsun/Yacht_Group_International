generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Inquiry {
  id        String   @id @default(cuid())
  firstName String
  lastName  String
  email     String
  phone     String?
  service   String
  message   String
  createdAt DateTime @default(now())
}
